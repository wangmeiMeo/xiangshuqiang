page {
  width: 100%;
}

.bg {
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: -100;
}

.doors {
  width: 92%;
  height: 80rpx;
  line-height: 80rpx;
  padding-left: 4%;
  padding-right: 4%;
  background: #fff;
  opacity: 0.8;
}

.doors text {
  font-size: 26rpx;
  color: #76dce3;
}
.bgyicon{
  width: 40rpx;
  margin-top: 20rpx;
  margin-right: 6rpx;
}
.logo {
  display: block;
  width: 100%;
  margin: 0 auto 10rpx;
  padding-top: 90rpx;
}
.er{
  display: block;
  width: 52%;
  margin: 0 auto;
  box-shadow: 2px 10px 20px #666;
  border:3px solid #76dce3;
  color:#474848;
}
.sao {
  display: block;
  text-align: center;
  color: #474848;
  font-size: 28rpx;
  margin-top: 16rpx;
}
.content text {
  display: block;
}

.title {
  display: block;
  text-align: center;
  color: #474848;
  background: #fff;
  opacity: 0.8;
  border-radius: 10rpx;
  height: 70rpx;
  line-height: 36px;
  font-size: 30rpx;
  width: 30%;
  margin: 30rpx auto 0;
  border: 1px solid #ccc;
}

.pass {
  width: 86%;
  font-size: 24rpx;
  line-height: 38rpx;
  margin: 40rpx auto;
  padding: 20rpx;
  text-indent: 2em;
  color: #474848;
  background: #fff;
  opacity: 0.8;
  border-radius: 20rpx;
  border: 1px solid #ccc;
}
